        <link rel="stylesheet" href="{{ asset('css/main.css') }}">
        {% block stylesheets %}{% endblock %}

    </head>
    <body>
        {{ include('includes/navigation.html.twig') }}
        <div class="container">
        {% block body %}{% endblock %}
        </div>

